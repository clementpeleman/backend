{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "naam": {
      "type": "string"
    },
    "beschrijving": {
      "type": "richtext"
    },
    "afbeeldingen": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "jaar": {
      "type": "date"
    },
    "slug": {
      "type": "uid",
      "targetField": "naam"
    },
    "samenwerkings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::samenwerking.samenwerking"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "Adres": {
      "type": "string"
    },
    "Link_Socialmedia": {
      "type": "string"
    },
    "korte_beschrijving": {
      "type": "string"
    },
    "Thumbnail": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
